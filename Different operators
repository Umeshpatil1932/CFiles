using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace different_operators
{
    class Program
    {
        static void Main(string[] args)
        {
            int a, b, c,d,e,f;
            Console.WriteLine("Enter first number: ");
            a = Convert.ToInt32(Console.ReadLine());

            Console.WriteLine("Enter second number: ");
            b = Convert.ToInt32(Console.ReadLine());

            c = a + b;
            Console.WriteLine("addition of a&b: " +c);

            d = a * b;
            Console.WriteLine("multiplication of a&b: " + d);

            e = a / b;
            Console.WriteLine("division of a&b: " + e);

            f = a % b;
            Console.WriteLine("modulation of a&b: " + f);

            Console.ReadLine();
        }
    }
}
